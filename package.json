{
  "name": "citz-imb-sp-gdx-bif",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fluentui/react": "^8.9.4",
    "@fluentui/react-hooks": "^8.1.1",
    "@material-ui/core": "^4.11.3",
    "@types/react-router-dom": "^5.1.7",
    "formik": "^2.2.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-iframe": "^1.8.0",
    "react-query": "^3.13.6",
    "react-scripts": "^4.0.3",
    "react-table": "^7.6.3",
    "typescript": "^4.2.4",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "proxy": "node ./api-server.js",
    "startServers": "concurrently --kill-others \"npm run proxy\" \"npm run start\"",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "devbuild": "react-scripts build && gulp dev",
    "uatbuild": "react-scripts build && gulp uat",
    "devuatbuild": "react-scripts build && gulp devuat"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.0",
    "@types/jest": "^26.0.20",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-query": "^1.1.2",
    "@types/react-table": "^7.0.29",
    "@types/sharepoint": "^2016.1.9",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "concurrently": "^6.0.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "lottie-web": "^5.7.8",
    "prettier": "^2.2.1",
    "sp-rest-proxy": "^3.0.5",
    "typescript": "^4.1.5"
  },
  "proxy": "http://localhost:8081",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
